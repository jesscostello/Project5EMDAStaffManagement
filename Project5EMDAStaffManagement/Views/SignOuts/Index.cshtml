@*@model IEnumerable<Project5EMDAStaffManagement.Models.SignOuts>*@
@using System.Collections
@model List<SignOuts>

@{
    ViewData["Title"] = "Index";
}
<div class="container">
    <h2>Staff Currently Out</h2><br /><br />

    @foreach (var item in ViewData["StaffOut"] as List<SignOuts>)
    {
        <p>
            @Html.DisplayFor(modelItem => item.StaffName)
            signed out at
            @Html.DisplayFor(modelItem => item.TimeOut)
            for the following reason:
            @Html.DisplayFor(modelItem => item.Reason)
            Session in office (hours) =
            @Html.DisplayFor(modelItem => item.HoursIn)
        </p>
    }
</div>